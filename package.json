{
  "name": "typescript-mcp-workshop",
  "version": "1.0.0",
  "type": "module",
  "description": "Typescript MCP Workshop",
  "scripts": {
    "setup": "node scripts/setup.js",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "dev": "pnpm fastmcp dev src/01-tools-only/server.ts",
    "inspect": "pnpm fastmcp inspect src/01-tools-only/server.ts",
    "example:server:01": "pnpm tsx --watch src/01-tools-only/server.ts",
    "example:client:01": "pnpm tsx --watch src/01-tools-only/client.ts",
    "example:server:02": "pnpm tsx --watch src/02-resources/server.ts",
    "example:client:02": "pnpm tsx --watch src/02-resources/client.ts",
    "example:server:03": "pnpm tsx --watch src/03-prompts/server.ts",
    "example:client:03": "pnpm tsx --watch src/03-prompts/client.ts",
    "example:server:04": "pnpm tsx --watch src/04-stdio-filesystem/server.ts",
    "example:client:04": "pnpm tsx --watch src/04-stdio-filesystem/client.ts",
    "example:user:05": "pnpm tsx --watch src/05-multi-server/user-server.ts",
    "example:payment:05": "pnpm tsx --watch src/05-multi-server/payment-server.ts",
    "example:server:05": "npm-run-all --parallel example:user:05 example:payment:05",
    "example:client:05": "pnpm tsx --watch src/05-multi-server/client.ts",
    "example:server:06": "pnpm tsx --watch src/06-auth-api-key/server.ts",
    "example:client:06": "pnpm tsx --watch src/06-auth-api-key/client.ts",
    "example:client:07": "pnpm tsx --watch src/07-playwright-demo/client.ts"
  },
  "author": "Jag Reehal <jag@jagreehal.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.22",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "ai": "^4.3.16",
    "dotenv": "^16.5.0",
    "fastmcp": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "ollama-ai-provider": "^1.2.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@total-typescript/ts-reset": "^0.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
